{
	"name": "clancats/container",
	"description": "ClanCats IoC Container.",
	"license": "MIT",
	"require": {
		"php": ">=7.1"
	},
    "require-dev": {
        "phpunit/phpunit": "7.0"
    },
	"autoload": {
        "psr-4": {
        	"ClanCats\\Container\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
        	"ClanCats\\Container\\Tests\\": "tests/"
        }
    },
	"minimum-stability": "dev",
    "scripts": {
        "post-autoload-dump": [
            "ClanCats\\Container\\ComposerContainerFileLoader::generateMap"
        ]
    }
}
